  "description": "Deployment scripts for L1BitcoinDepositor and L2BitcoinDepositor contracts on Sepolia (Arbitrum and Mainnet)",
  "main": "index.js",
  "scripts": {
    "build": "hardhat compile",
    "deploy:ethereumSepolia": "hardhat run scripts/deploy.ts --network ethereumSepolia",
    "deploy:arbitrumSepolia": "hardhat run scripts/deploy.ts --network arbitrumSepolia",
    "verify:ethereumSepolia": "hardhat verify --network ethereumSepolia",
    "verify:arbitrumSepolia": "hardhat verify --network arbitrumSepolia"
  },
